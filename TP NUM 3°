#include <bits/stdc++.h>
using namespace std; 
int ContarLetraA (string f) ;

int main ()
{ int contar; string frase;

cout<<"Ingrese frase:"<<endl; 
getline (cin, frase); 
contar=ContarLetraA (frase);
cout<<"La cantidad de vocales A es:" <<contar<<endl; 
return 0;
} 
int ContarLetraA (string f) {
int i;
int contar=0; 
for (i=0; i<f.size();i++){
      if (f.[i]=='A'|| f.[i]=='a') 
      {contar++;}

}

return contar;

}
